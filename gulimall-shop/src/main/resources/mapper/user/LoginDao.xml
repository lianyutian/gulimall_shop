<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atguigu.gulimall.shop.dao.user.LoginDao">
    <select id="queryUser" resultType="com.atguigu.gulimall.shop.model.User">
        select userName from sys_user where username = #{userName} and password = #{passWord}
    </select>
</mapper>